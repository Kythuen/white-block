{
  "name": "@white-block/resolver",
  "version": "0.0.38",
  "description": "unplugin-vue-components resolver for WhiteBlock",
  "keywords": [
    "white-block",
    "unplugin-vue-components",
    "resolver"
  ],
  "author": {
    "name": "ephemeras",
    "email": "ephemeras@qq.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "test": "echo \"package resolver ignore\"",
    "test:coverage": "echo \"package resolver coverage ignore\""
  },
  "peerDependencies": {
    "unplugin-vue-components": "^0.25.2"
  }
}
